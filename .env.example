# 佐渡で食えっちゃ - 環境変数設定例
# 実際に使用する際はこのファイルをコピーして .env または .env.development などにリネームして使用してください

# Google Maps API 関連設定
VITE_GOOGLE_API_KEY=your_google_api_key_here
VITE_GOOGLE_MAPS_MAP_ID=your_map_id_here
VITE_GOOGLE_API_KEY_RESTRICTIONS=false
VITE_GOOGLE_MAPS_VERSION=weekly
VITE_GOOGLE_SPREADSHEET_ID=your_spreadsheet_id_here

# ロギング設定
VITE_LOG_LEVEL=info
VITE_DEBUG_MODE=false

# 機能フラグとアプリケーション設定
VITE_ENABLE_MARKER_CLUSTERING=true
VITE_ENABLE_OFFLINE_MODE=false

# アプリケーション情報
VITE_APP_NAME=佐渡で食えっちゃ
VITE_APP_SHORT_NAME=佐渡マップ
VITE_ENABLE_PWA=false
