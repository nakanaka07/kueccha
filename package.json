{
  "name": "sadodekueccha",
  "description": "佐渡島内の飲食店、駐車場、公共トイレの位置情報を網羅するマップアプリケーション",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "// 開発関連": "----------------------------",
    "dev": "cross-env NODE_ENV=development vite",
    "predev": "node scripts/validate-env.js",
    "dev:verbose": "cross-env NODE_ENV=development VITE_LOG_LEVEL=debug vite",
    "dev:quiet": "cross-env NODE_ENV=development VITE_LOG_LEVEL=warn vite",
    "dev:debugmode": "cross-env NODE_ENV=development VITE_DEBUG_MODE=true VITE_LOG_LEVEL=debug vite",
    "preview": "vite preview",
    "start": "vite preview",
    "// ビルド関連": "----------------------------",
    "build": "vite build",
    "prebuild": "node scripts/validate-env.js",
    "build:dev": "cross-env NODE_ENV=development vite build",
    "build:prod": "cross-env NODE_ENV=production vite build",
    "build:analyze": "cross-env ANALYZE=true MINIFY=true vite build",
    "build:report": "cross-env ANALYZE=true REPORT_JSON=true vite build",
    "build:minify": "cross-env NODE_ENV=production MINIFY=true vite build",
    "build:pwa": "cross-env NODE_ENV=production ENABLE_PWA=true vite build",
    "build:images": "cross-env NODE_ENV=production OPTIMIZE_IMAGES=true vite build",
    "build:offline": "cross-env NODE_ENV=production ENABLE_PWA=true ENABLE_OFFLINE=true vite build",
    "analyze": "cross-env ANALYZE=true vite build",
    "// コード品質": "----------------------------",
    "format": "prettier --write .",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --ignore-pattern \"dev-dist/**\" --fix --config eslint.config.js",
    "lint:unused": "pnpm dlx depcheck",
    "lint:all": "pnpm lint && pnpm format && pnpm typecheck",
    "typecheck": "tsc --noEmit",
    "typecheck:env": "tsc --noEmit src/utils/env.ts",
    "typecheck:ci": "cross-env CI=true tsc --noEmit",
    "// テスト関連": "----------------------------",
    "test": "vitest run",
    "pretest": "node scripts/validate-env.js",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:a11y": "vitest run a11y",
    "test:ui": "vitest --ui",
    "test:ci": "cross-env CI=true VITE_LOG_LEVEL=error vitest run",
    "// パフォーマンス関連": "----------------------------",
    "analyze:bundle": "cross-env ANALYZE=true vite build",
    "analyze:runtime": "cross-env NODE_ENV=development MEASURE_PERFORMANCE=true vite",
    "perf:analyze": "cross-env NODE_ENV=development MEASURE_PERFORMANCE=detailed vite",
    "perf:report": "node scripts/performance-report.js",
    "perf:monitor": "cross-env NODE_ENV=development MEASURE_PERFORMANCE=true VITE_LOG_LEVEL=warn vite",
    "// プロジェクト管理": "----------------------------",
    "prepare": "husky install",
    "preprod": "npm run lint:all && npm run test && npm run build:prod",
    "prod:verify": "node scripts/verify-build.js",
    "clean": "rimraf node_modules pnpm-lock.yaml dist dev-dist stats.html .husky tsconfig.tsbuildinfo && pnpm store prune",
    "docs": "npx serve docs",
    "setup": "node scripts/dev-setup.js",
    "// 環境変数検証": "----------------------------",
    "validate:env": "node scripts/validate-env.js",
    "validate:env:ci": "cross-env CI=true node scripts/validate-env.js",
    "validate:env:prod": "cross-env NODE_ENV=production node scripts/validate-env.js",
    "validate:env:all": "node scripts/comprehensive-env-check.js",
    "// CI/CD連携": "----------------------------",
    "ci": "pnpm validate:env:ci && pnpm typecheck:ci && pnpm lint && pnpm test:ci",
    "ci:build": "pnpm ci && pnpm build:prod",
    "ci:deploy": "pnpm ci:build && node scripts/verify-build.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@googlemaps/js-api-loader": "^1.16.8",
    "@googlemaps/markerclusterer": "^2.5.3",
    "@mui/material": "^7.0.1",
    "@react-google-maps/api": "^2.20.6",
    "@types/dotenv": "^8.2.3",
    "axios": "^1.8.4",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "idb": "^8.0.2",
    "lodash-es": "^4.17.21",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.1.1",
    "@vitest/ui": "^3.1.1",
    "cross-env": "^7.0.3",
    "depcheck": "^1.4.7",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.3.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "sort-package-json": "^3.0.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.2.5",
    "vite-plugin-checker": "^0.9.1",
    "vite-plugin-chunk-split": "^0.5.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-inspect": "^11.0.0",
    "vite-plugin-mkcert": "^1.17.8",
    "vite-plugin-pwa": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1",
    "whatwg-fetch": "^3.6.20"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "ignoredBuiltDependencies": [
      "cwebp-bin",
      "gifsicle",
      "jpegtran-bin",
      "mozjpeg",
      "optipng-bin",
      "pngquant-bin"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --ignore-path .gitignore",
      "prettier --write"
    ],
    "*.{json,css,md,html}": "prettier --write",
    "package.json": "sort-package-json"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all",
    "last 2 versions",
    "not ie <= 11"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/user/kueccha"
  },
  "keywords": [
    "sadodekueccha",
    "map",
    "restaurants",
    "sado-island",
    "google-maps",
    "typescript",
    "react"
  ]
}
