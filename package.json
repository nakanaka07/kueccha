{
  "name": "sadodekueccha",
  "description": "佐渡島内の飲食店、駐車場、公共トイレの位置情報を網羅するマップアプリケーション",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "license": "UNLICENSED",
  "packageManager": "pnpm@10.8.0",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "// 基本コマンド": "----------------------------",
    "dev": "cross-env NODE_ENV=development vite",
    "predev": "node scripts/comprehensive-env-check.js",
    "build": "vite build",
    "prebuild": "node scripts/comprehensive-env-check.js",
    "preview": "vite preview",
    "test": "vitest run",
    "pretest": "node scripts/comprehensive-env-check.js",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "build:dts": "tsc --project tsconfig.node.json",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx src --fix",
    "// 拡張コマンド": "----------------------------",
    "start": "vite preview",
    "dev:config": "cross-env NODE_ENV=development VITE_LOG_LEVEL=debug VITE_DEBUG_MODE=false VITE_MEASURE_MAP_PERFORMANCE=false vite",
    "build:config": "cross-env NODE_ENV=production VITE_OPTIMIZE_MAPS=true VITE_ENABLE_MARKER_CLUSTERING=true ENABLE_PWA=false ENABLE_OFFLINE=false vite build",
    "test:config": "cross-env VITE_LOG_LEVEL=error vitest run",
    "lint:all": "pnpm lint:fix && pnpm format && pnpm typecheck",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "// 分析と最適化": "----------------------------",
    "analyze:bundle": "cross-env ANALYZE=true MINIFY=true vite build",
    "analyze:perf": "node scripts/performance-report.js",
    "analyze:deps": "pnpm dlx depcheck",
    "// プロジェクト管理": "----------------------------",
    "prepare": "husky install",
    "clean": "rimraf node_modules pnpm-lock.yaml dist dist-node dev-dist stats.html .husky tsconfig.tsbuildinfo && pnpm store prune",
    "validate:env": "node scripts/comprehensive-env-check.js",
    "docs": "npx serve docs",
    "// CI/CD連携": "----------------------------",
    "ci": "cross-env CI=true pnpm validate:env && pnpm typecheck && pnpm lint && pnpm test:config",
    "ci:build": "pnpm ci && cross-env NODE_ENV=production vite build",
    "ci:deploy": "pnpm ci:build && node scripts/verify-build.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@googlemaps/js-api-loader": "^1.16.8",
    "@googlemaps/markerclusterer": "^2.5.3",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@react-google-maps/api": "^2.20.6",
    "axios": "^1.8.4",
    "csv-parse": "^5.6.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "idb": "^8.0.2",
    "immer": "^10.1.1",
    "lodash-es": "^4.17.21",
    "query-string": "^9.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.0",
    "uuid": "^11.1.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "sort-package-json": "^3.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "vite": "^6.2.5",
    "vite-plugin-checker": "^0.9.1",
    "vite-plugin-compression2": "^1.3.3",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-pwa": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.1",
    "whatwg-fetch": "^3.6.20"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "ignoredBuiltDependencies": [
      "cwebp-bin",
      "gifsicle",
      "jpegtran-bin",
      "mozjpeg",
      "optipng-bin",
      "pngquant-bin"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --ignore-path .gitignore",
      "prettier --write"
    ],
    "*.{json,css,md,html}": "prettier --write",
    "package.json": "sort-package-json"
  },
  "browserslist": [
    ">0.5%",
    "last 2 versions",
    "not dead",
    "not IE 11"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "sadodekueccha",
    "map",
    "restaurants",
    "sado-island",
    "google-maps",
    "typescript",
    "react"
  ]
}
