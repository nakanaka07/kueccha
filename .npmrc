registry=https://registry.npmjs.org/ # npmパッケージのレジストリを指定

# キャッシュの有効期限を設定（デフォルトは10分）
cache-min=10

# プロキシ設定（必要に応じてコメントアウトを解除して使用）
# proxy=http://proxy.example.com:8080
# https-proxy=http://proxy.example.com:8080

# プライベートレジストリの設定（必要に応じて追加）
# @scope:registry=https://registry.your-company.com/

# ログレベルの設定（silent, error, warn, info, verbose, silly）
loglevel=warn